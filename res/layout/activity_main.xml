<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bad_chicken"
    tools:context="com.insa.cursoandroid.MainActivity"
    tools:ignore="MergeRootFrame" >

    <Button
        android:id="@+id/btnNavigator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:onClick="call2intent"
        android:text="@string/strNavigator" />

    <Button
        android:id="@+id/btnCall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/btnNavigator"
        android:onClick="call2intent"
        android:text="@string/strMakeCall" />

    <Button
        android:id="@+id/btnCallContact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/btnCall"
        android:onClick="call2intent"
        android:text="@string/strContactCall" />

</RelativeLayout>
